{
  "name": "sound-finder",
  "version": "0.0.0",
  "description": "build a community around music",
  "main": "index.js",
  "scripts": {
    "prepublish": "mkdir -p dist && npm run compile && npm run sass",
    "compile": "browserify -e src/js/index.js -d -t [ 6to5ify --sourceMapRelative .] -o dist/bundle.js",
    "watch:js": "fsmonitor -d src/js/ npm run compile",
    "watch:sass": "fsmonitor -d src/sass/ npm run sass",
    "sass": "node-sass -r --include-path node_modules/scss-utils/ src/sass/style.scss -o dist/",
    "img": "imageoptim -d src/img/",
    "predev": "npm run prepublish",
    "dev": "parallelshell 'live-server' 'npm run watch:js' 'npm run watch:sass'"
  },
  "author": "Paul C Pederson <paul.c.pederson@gmail.com> (http://paulcpederson.com/)",
  "license": "ISC",
  "devDependencies": {
    "6to5ify": "^4.1.0",
    "array-flatten": "^1.0.2",
    "browserify": "^8.1.3",
    "fsmonitor": "^0.2.4",
    "hogan.js": "^3.0.2",
    "imageoptim-cli": "^1.11.4",
    "live-server": "^0.6.3",
    "node-sass": "^2.0.0-beta",
    "parallelshell": "^1.0.4",
    "scss-utils": "*"
  }
}
