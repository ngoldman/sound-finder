{
  "name": "sound-finder",
  "version": "0.0.0",
  "description": "build a community around music",
  "main": "index.js",
  "scripts": {
    "compile": "browserify -e src/index.js -d -t [ 6to5ify --sourceMapRelative .] -o dist/bundle.js",
    "watch:js": "fsmonitor -d src/ npm run compile",
    "dev": "npm run compile && parallelshell 'npm run watch:js' 'live-server'"
  },
  "author": "Paul C Pederson <paul.c.pederson@gmail.com> (http://paulcpederson.com/)",
  "license": "ISC",
  "devDependencies": {
    "6to5ify": "^4.1.0",
    "browserify": "^8.1.3",
    "live-server": "^0.6.3",
    "parallelshell": "^1.0.4"
  }
}
