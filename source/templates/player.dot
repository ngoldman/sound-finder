<button class="player-control prev"></button>
<button class="player-control pause-play {{?it.isPlaying}}isPlaying{{?}}"></button>
<button class="player-control next"></button>

<span class="elapsed">{{=it.elapsed || '0:00'}}</span>
<div class="progress-bar"><div class="progress-bar-fill" style="width: {{=it.progress || 0}}%;"></div></div>
<span class="remaining">{{=it.remaining || ''}}</span>

<img src="{{=it.artwork_url}}" alt="{{=it.title}}" class="album-thumb">
<a class="song-title" data-id="{{=it.id}}" href="{{=it.permalink_url}}">{{=it.title}}</a>
<a class="username" data-id="{{=it.user.id}}">{{=it.user.username}}</a>
